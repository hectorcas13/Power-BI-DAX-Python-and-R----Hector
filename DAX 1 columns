//Calculate yes responses in one column

var __yesCount =
	CALCULATE(
		COUNTROWS( table ),
		KEEPFILTERS( table[column] = "yes" )
	)

//CALCULATE THE NUMBER OF ROWS WITH NO BLANKS IN ONE COLUMN
var __totalCount = 
	CALCULATE(
		COUNTROWS( table ),
		KEEPFILTERS(
			NOT ISBLANK( table[column] )
		)
	)
	
//CALCULATE THE NUMBER OF ROWS WITH TRUE
	VAR COUNT_EXT = ROUNDUP(INT(CALCULATE(COUNTROWS(Extension), Extension[Extension approved] = "TRUE")),5)
	return IF(ISBLANK(COUNT_EXT),"0",ROUNDUP(INT(COUNT_EXT),5))

